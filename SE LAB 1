        "def fibonacci(n):\n",
        "    if n <= 0:\n",
        "        return \"Input should be a positive integer.\"\n",
        "    a, b = 0, 1\n",
        "    for _ in range(1, n):\n",
        "        a, b = b, a + b\n",
        "    return a\n",
        "\n",
        "n = int(input(\"Enter a positive integer: \"))\n",
        "print(f\"The {n}th Fibonacci number is: {fibonacci(n)}\")"
      ]
